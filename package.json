{
  "name": "csgo-config",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://mauricioromagnollo.github.io/csgo-config",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"{src,test}/**/*.{ts,js,tsx,jsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest --passWithNoTests",
    "test:unit": "npm run test -- --run test/**/*.spec.ts",
    "test:integration": "npm run test -- --run test/**/*.test.ts",
    "test:watch": "npm run test -- dev test/**/*.spec.ts",
    "test:coverage": "npm run test -- --coverage --run test/**/*.spec.ts",
    "test:ci": "npm run test -- related --run test/**/*.spec.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mauricioromagnollo/eslint-config": "^2.2.2",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.15.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "0.29.2",
    "eslint": "^8.35.0",
    "jsdom": "21.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "0.29.2"
  }
}
